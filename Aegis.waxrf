<?xml version="1.0"?>
<!--
	____ ____ ____ ____    ____ ____ ____ ____ ____ ____ _
	|=== |==< _][_ [___    |==< _][_  ||  |=== _/__ |=== |___

	Layout for XJOB processor frontend project.
	Includes graphics.
	Arr.
	
	January 3, 2007
-->
<!--
	Custom Notebook for settings
	NOTE: Hierarchy of name elements is:

	nb
	+-output
	| +-Client
	| | +-(string) Text
	| +-Case
	| | +-(string) Text
	| +-VolumeName
	| | +-(string) Text
	| +-Media
	|   +-(string) Text
	+-page
	| +-(bool) SameAsCapturedBates
	| +-(bool) Custom
	| +-(string) CustomName
	+-file
	| +-(bool) SameAsPageName
	| +-(bool) SameAsCapturedFilename
	| +-(bool) Custom
	| +-(string) CustomName
	+-directory
	| +-(bool) Box
	| +-(bool) JFS
	| +-(bool) SFCityAttorney
	| +-(bool) Custom
	| +-User
	|   +-(string) Text
	+-loadfiles
		+-(bool) IPRO
		+-(bool) SummationI
		+-(bool) SummationV
		+-(bool) Opticon
		+-(bool) Lotus
		+-(bool) DocLevel
		+-(bool) pageLevel
-->
<resource>
	<Image name="cdicon"></Image>
	<Image name="dvdicon"></Image>
	<Image name="diricon"></Image>
	<Image name="hdicon"></Image>
	<Image name="cd16"></Image>
	<Image name="hd16"></Image>
	<Image name="dir16"></Image>
	<Image name="help16"></Image>
	<ImageList name="iolist" width="48" height="48">
		<Item name="cd" bmp="cdicon" />
		<Item name="dvd" bmp="dvdicon" />
		<Item name="hd" bmp="hdicon" />
		<Item name="dir" bmp="diricon" />
	</ImageList>
	<Dialog name="volhelpdlg">
		<HTMLWindow name="html" _expand="b" fullrepaint="0" />
	</Dialog>
	<NoteBook name="nb">
		<NotebookPage name="output" text="Job Settings">
			<GroupBox name="output" text="Basic Job Specifications">
				<HorizontalPanel name="header">
					<VerticalPanel _expand="h" _border="6">
						<Label text="Here are some important job-related settings." _border="0" _align="bottom" />
						<Label text="It is advisable to fill these in before proceeding with the rest of the job." _border="0" _align="bottom" />
					</VerticalPanel>
					<BitmapButton bmp="help16" name="volhelp" _border="8" />
				</HorizontalPanel>
				<Line _border="6" _expand="h" />
				<FlexGridPanel name="container" cols="2" rows="4" gcols="1," _expand="h" _border="3">
					<Label text="Client name:" _align="bottom" _border="3" />
					<TextBox name="Client" />
					
					<Label text="Case name:" _align="bottom" _border="3" />
					<TextBox name="Case" />

					<Label text="Volume name:" _align="bottom" _border="3" />
					<TextBox name="VolumeMask" text="Aegis001" />

					<Label text="Output media size:" _align="bottom" _border="3" />
					<BitmapComboBox name="Media" readonly="true">
						<Item bmp="cd16" text="CD-ROM (610 Mb)" />
						<Item bmp="cd16" text="DVD-ROM (4.2 Gb)" />
						<Item bmp="hd16" text="Unlimited" />
					</BitmapComboBox>
				</FlexGridPanel>
			</GroupBox>
		</NotebookPage>
		<NotebookPage name="page" text="Numbering">
			<GroupBox name="page" text="Numbering Settings">
				<VerticalPanel _expand="h" _border="6">
					<Label text="You can select the way numbering will be assigned to pages." _border="0" _align="bottom" />
					<Label text="Please be aware that ID numbers are not supported for non-export jobs." _border="0" _align="bottom" />
				</VerticalPanel>
				<Line _border="6" _expand="h" />
				<FlexGridPanel name="container" cols="2" rows="3" gcols="1," _expand="h" _border="3">
					<RadioButton name="SameAsCapturedBates" text="Use captured Bates number" _border="3" selected="true" />
					<Label text="" />
					
					<RadioButton name="SameAsIDNumber" text="Use ID number" _border="3" />
					<Label text="" />
					
					<RadioButton name="Custom" text="Use custom page name: " _border="3" />
					<TextBox name="PageMask" _border="5" />
				</FlexGridPanel>
			</GroupBox>
		</NotebookPage>
		<NotebookPage name="file" text="File Naming">
			<GroupBox name="file" text="Filesystem Output Name Convention">
				<VerticalPanel _expand="h" _border="6">
					<Label text="You can select the way files will be named in the final product." _border="0" _align="bottom" />
					<Label text="Note that the file extension is provided by the original file." _border="0" _align="bottom" />
				</VerticalPanel>
				<Line _border="6" _expand="h" />
				<FlexGridPanel name="container" cols="2" rows="3" gcols="1," _expand="h" _border="3">
					<RadioButton name="SameAsPageName" text="Match to page name" _border="3" selected="true" />
					<Label text="" />

					<RadioButton name="SameAsCapturedFilename" text="Use captured file name" _border="3" />
					<Label text="" />

					<RadioButton name="Custom" text="Use custom file name: " _border="3" />
					<TextBox name="FileMask" _border="5" />
				</FlexGridPanel>
			</GroupBox>
		</NotebookPage>
		<NotebookPage name="directory" text="Directory">
			<GroupBox name="directory" text="Filesystem Directory Structure">
				<VerticalPanel _expand="h" _border="6">
					<Label text="You can specify the directory structure of the deliverable." _border="0" _align="bottom" />
				</VerticalPanel>
				<Line _border="6" _expand="h" />
				<FlexGridPanel name="container" cols="1" rows="5" _expand="h" _border="3">
					<RadioButton name="Box" text="Use box-level boundaries for directories" _border="3" selected="true" />

					<RadioButton name="JFS" text="Use Litigator's Notebook (JFS) structure" _border="3" />

					<RadioButton name="Custom" text="Use a number-limited structure" _border="3" />

					<HorizontalPanel name="User" _expand="h" _border="3">
						<Label _align="center" text="Images per directory: " />
						<TextBox name="Text" />
					</HorizontalPanel>

					<CheckBox name="NameDirectoryAfterFirstDocument" text="Name directory after first bates" _border="5" />
				</FlexGridPanel>
			</GroupBox>
		</NotebookPage>
		<NotebookPage name="loadfiles" text="Loadfiles">
			<GroupBox name="loadfiles" text="Loadfile and Metadata Inclusion">
				<VerticalPanel _expand="h" _border="6">
					<Label text="You can select loadfiles and metadata collections to include with the deliverable." _border="0" _align="bottom" />
					<Label text="Please note that Summation and JFS files will change directory structure." _border="0" _align="bottom" />
				</VerticalPanel>
				<Line _border="6" _expand="h" />
				<FlexGridPanel name="container" cols="2" rows="4" gcols="0,1" _expand="h" _border="3">
					<CheckBox name="IPRO" text="IPRO (.lfp) load file" _border="3" />
					<CheckBox name="Opticon" text="Opticon load file" _border="3" />

					<CheckBox name="SummationI" text="Summation load file" _border="3" />
					<CheckBox name="SummationV" text="Summation Volume load file" _border="3" />

					<CheckBox name="DocLevel" text="Document Level file" _border="3" />
					<CheckBox name="PageLevel" text="Page Level file" _border="3" />

					<CheckBox name="JFS" text="Litigator's Notebook (JFS) file" _border="3" />
					<Label text="" />
				</FlexGridPanel>
			</GroupBox>
		</NotebookPage>
		<NotebookPage name="operations" text="Operations">
			<GroupBox name="operations" text="Local and Network Distributed Operations">
				<VerticalPanel _expand="h" _border="6">
					<Label text="You can select some common tasks to be run on each document and page." _border="0" _align="bottom" />
					<Label text="If an Ephemerol server is found on the network, it will help processing." _border="0" _align="bottom" />
				</VerticalPanel>
				<Line _border="6" _expand="h" />
				<FlexGridPanel name="container" cols="2" rows="2" gcols="1," _expand="h" _border="3">
					<CheckBox name="OCR" text="Run optical character recognition" _border="3" _align="top" />
					<GroupBox text="OCR Options" _expand="h">
						<CheckBox name="AutoRotate" text="Rotate images for OCR" _border="3" />
						<CheckBox name="AutoDeskew" text="Deskew images for OCR" _border="3" />
					</GroupBox>

					<CheckBox name="Embed" text="Embed text for each page" _border="3" />
					<GroupBox text="Embedding Options (XJOB Syntax)" _expand="h">
						<TextBox name="Text" _expand="h" text="page,bottom,right,bates,,;page,bottom,center,confidentiality,," />
					</GroupBox>
				</FlexGridPanel>
			</GroupBox>
		</NotebookPage>
		<NotebookPage name="about" text="About">
			<GroupBox name="about" text="About Aegis">
				<HTMLWindow name="html" _expand="b" fullrepaint="0" />
			</GroupBox>
		</NotebookPage>
	</NoteBook>
</resource>
